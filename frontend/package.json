{
  "name": "my-razzle-app",
  "version": "4.0.4",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "format": "prettier --write \\\"src/**/**.{ts,tsx,js,jsx,json}\\\"",
    "commit": "git-cz",
    "lint": "eslint --fix src"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@emotion/css": "^11.1.3",
    "antd": "^4.16.2",
    "cross-fetch": "^3.1.4",
    "effector": "^21.8.11",
    "effector-inspector": "^0.5.0",
    "effector-logger": "^0.10.0",
    "effector-react": "^21.3.2",
    "effector-root": "^1.3.0",
    "eslint": "^7.28.0",
    "express": "^4.17.1",
    "ky": "^0.25.1",
    "nanoid": "^3.1.23",
    "patronum": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "runtypes": "^6.3.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@eslint-kit/eslint-config-base": "^4.0.0",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-prettier": "^3.0.0",
    "@eslint-kit/eslint-config-react": "^3.0.0",
    "@eslint-kit/eslint-config-typescript": "^5.0.1",
    "@types/express": "^4.17.12",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router": "^5.1.14",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-preset-razzle": "4.0.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.3.1",
    "razzle": "^4.0.4",
    "razzle-dev-utils": "^4.0.4",
    "tslint": "^6.1.3",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
