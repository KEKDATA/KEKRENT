{
  "name": "razzle-examples-with-typescript",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "commit": "git-cz",
    "lint": "eslint --fix ./src"
  },
  "engines": {
    "node": ">=14.0.0 <=15.2.0"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "cross-fetch": "^3.1.4",
    "effector": "^21.8.11",
    "effector-inspector": "^0.5.0",
    "effector-logger": "^0.10.0",
    "effector-react": "^21.3.2",
    "effector-root": "^1.3.0",
    "express": "^4.17.1",
    "ky": "^0.25.1",
    "ky-universal": "^0.8.2",
    "patronum": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "reakit": "^1.3.8",
    "runtypes": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@eslint-kit/eslint-config-base": "^4.0.0",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-prettier": "^3.0.0",
    "@eslint-kit/eslint-config-react": "^3.0.0",
    "@eslint-kit/eslint-config-typescript": "^5.0.1",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router": "^5.1.14",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-plugin-styled-components": "^1.12.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "razzle": "^3.3.8",
    "razzle-plugin-typescript": "^3.3.8",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
